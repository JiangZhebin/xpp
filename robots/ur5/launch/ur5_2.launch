<launch>
  
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="ur5_rviz_urdf_robot_description" command="$(find xacro)/xacro  '$(find ur5)/urdf/ur5.urdf'"/>
  <param name="ur5_rviz_urdf_robot_description_semantic" command="$(find xacro)/xacro  '$(find ur5)/ur5_moveit_config/config/ur5.srdf'"/>

  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_ur5_2" pkg="ur5" type="urdf_visualizer_ur5_2" output="screen"/>
     <rosparam command="load" file="$(find ur5)/ur5_moveit_config/config/kinematics.yaml" />
</launch>
